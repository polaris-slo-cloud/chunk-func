    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch extract-successes",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "preLaunchTask": "build-extract-successes"
        },
        {
          "name": "Docker Debug extract-successes",
          "type": "docker",
          "request": "launch",
          "preLaunchTask": "docker-run: debug",
          "platform": "node",
          "node": {
            "remoteRoot": "/usr/chunkfunc"
          }
        }
    ]
}
